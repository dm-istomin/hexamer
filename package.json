{
  "name": "hexamer",
  "version": "0.1.0",
  "description": "A custom browser with Vi-like keybindings, built on top of Electron.",
  "main": "main.js",
  "scripts": {
    "test": "mocha-webpack --webpack-config config/webpack.test.config.js spec/specs.js",
    "start": "HEXAMER_ENV=dev ./node_modules/.bin/electron .",
    "build": "./node_modules/.bin/webpack --config ./config/webpack.config.js",
    "watch": "./node_modules/.bin/webpack-dev-server --hot --inline --config ./config/webpack.config.js"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dm-istomin/hexamer.git"
  },
  "author": "Dmitriy Istomin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dm-istomin/hexamer/issues"
  },
  "homepage": "https://github.com/dm-istomin/hexamer#readme",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^1.2.5",
    "mocha": "^3.0.2",
    "mocha-webpack": "^0.6.0",
    "node-sass": "^3.8.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
